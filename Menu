-- Scripts:

local function EYEYRD_fake_script() -- Title.Script 
	local script = Instance.new('Script', Title)

	local text = script.Parent
	local add = 10
	wait(1)
	local k = 1
	while k <= 255 do
		text.TextColor3 = Color3.new(k/255,0/255,0/255)
		k = k + add
		wait()
	end
	while true do
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255,k/255,0/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255 - k/255,255/255,0/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(0/255,255/255,k/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(0/255,255/255 - k/255,255/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(k/255,0/255,255/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255,0/255,255/255 - k/255)
			k = k + add
			wait()
		end
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255 - k/255,0/255,0/255)
			k = k + add
			wait()
		end
	end
end
coroutine.wrap(EYEYRD_fake_script)()
local function PPRYR_fake_script() -- MenuFrame.LocalScript 
	local script = Instance.new('LocalScript', MenuFrame)

	local Plr = game.Players.LocalPlayer
	
	Plr:GetMouse() .KeyDown:Connect(function(L)
		if L =="l" then
			script.Parent.Visible = true
		end
	end)
	
end
coroutine.wrap(PPRYR_fake_script)()
local function COFXNT_fake_script() -- MenuFrame.LocalScript 
	local script = Instance.new('LocalScript', MenuFrame)

	local Plr = game.Players.LocalPlayer
	
	Plr:GetMouse() .KeyDown:Connect(function(P)
		if P =="p" then
			script.Parent.Visible = false
		end
	end)
	
end
coroutine.wrap(COFXNT_fake_script)()
------  ez hot
PC2.MouseButton1Down:connect(function()
	LoginFrame.Visible = false
	PC2.Visible = false --- false = hide 
	MenuFrame.Visible = true--- true = dont hide 
	BigFrame.Visible = true
end)

Add.MouseButton1Down:connect(function()
	MenuFrame.Visible = true
	Exit.Visible = true
	Add.Visible = false
end)

Exit.MouseButton1Down:connect(function()
	MenuFrame.Visible = false
	Add.Visible = true
	Exit.Visible = false
end)

Pan.MouseButton1Down:connect(function()
	PanelFrame.Visible = true
	DonoFrame.Visible = false
end)

Don.MouseButton1Down:connect(function()
	PanelFrame.Visible = false
	DonoFrame.Visible = true
end)
