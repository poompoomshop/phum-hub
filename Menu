local function TQET_fake_script() -- hide.Script 
	local script = Instance.new('Script', hide)

	script.Parent.MouseButton1Click:Connect(function()
		local XlFrame = script.Parent.Parent.Frame
		local button = script.Parent
		if XlFrame.Visible == false then
			XlFrame.Visible = true
			button.Text = "<"
		else
			XlFrame.Visible = false
			button.Text = ">"
		end
	end)
end
coroutine.wrap(TQET_fake_script)()

-- Other script gay poom
craft.MouseButton1Down:connect(function() 
	local item = craftitem.Text
	local CI = item
	local Event = game:GetService("ReplicatedStorage").Events.CraftItem
	Event:FireServer(CI)
end)
