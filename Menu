-- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local TdFrame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local UIGradient = Instance.new("UIGradient")
local TextButton = Instance.new("TextButton")
local UICorner_2 = Instance.new("UICorner")

--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

TdFrame.Name = "TdFrame"
TdFrame.Parent = ScreenGui
TdFrame.BackgroundColor3 = Color3.fromRGB(43, 43, 43)
TdFrame.Position = UDim2.new(0.453781545, 0, 0.393028855, 0)
TdFrame.Size = UDim2.new(0.139625087, 0, 0.319711506, 0)
TdFrame.Active = true
TdFrame.Draggable = true

UICorner.Parent = TdFrame

UIGradient.Parent = TdFrame

TextButton.Parent = TdFrame
TextButton.BackgroundColor3 = Color3.fromRGB(123, 255, 15)
TextButton.Position = UDim2.new(0.0462962911, 0, 0.334586501, 0)
TextButton.Size = UDim2.new(0.902777731, 0, 0.270676732, 0)
TextButton.Font = Enum.Font.Bangers
TextButton.Text = "Activate!"
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextScaled = true
TextButton.TextSize = 14.000
TextButton.TextWrapped = true

UICorner_2.Parent = TextButton

-- Scripts:

local function XCRCW_fake_script() -- TdFrame.LocalScript 
	local script = Instance.new('LocalScript', TdFrame)

	local Plr = game.Players.LocalPlayer
	
	Plr:GetMouse().KeyDown:Connect(function(L)
		if L == "l" then
			script.Parent.Visible = false
		end
		
	end)
	
end
coroutine.wrap(XCRCW_fake_script)()
local function JINVVT_fake_script() -- TdFrame.LocalScript 
	local script = Instance.new('LocalScript', TdFrame)

	local Plr = game.Players.LocalPlayer
	
	Plr:GetMouse().KeyDown:Connect(function(L)
		if L == "l" then
			script.Parent.Visible = true
		end
		
	end)
	
end
coroutine.wrap(JINVVT_fake_script)()

TextButton.MouseButton1Down:connect(function()
	game.Lighting.FogEnd = 999999988484154753734934528
	game.Lighting.FogStart = 0
end)
