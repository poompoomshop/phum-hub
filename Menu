local function RQMV_fake_script() -- Exit.Script 
	local script = Instance.new('Script', Exit)

	script.Parent.MouseButton1Click:Connect(function()
		local XlFrame = script.Parent.Parent.XlFrame
		local Exit = script.Parent
		if XlFrame.Visible == true then
			XlFrame.Visible = false
			Exit.Text = "x"
		else
			XlFrame.Visible = true
			Exit.Text = "-"
		end
	end)
end
coroutine.wrap(RQMV_fake_script)()

-- Other script gay poom
craft.MouseButton1Down:connect(function() 
	local item = craftitem.Text
	local CI = item
	local Event = game:GetService("ReplicatedStorage").Events.CraftItem
	Event:FireServer(CI)
end)

anti.MouseButton1Down:connect(function() 

	loadstring(game:HttpGet("https://pastebin.com/raw/gsxvWvnj"))()

end)

TextButton.MouseButton1Down:connect(function()
	if password.Text =="Ezx34" or "Ex22" then
		LoginFrame.Visible = false
		XlFrame.Visible = true
		DisFrame.Visible = true
	end
end)
