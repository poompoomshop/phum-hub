ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

LoginFrame.Name = "LoginFrame"
LoginFrame.Parent = ScreenGui
LoginFrame.BackgroundColor3 = Color3.fromRGB(58, 58, 58)
LoginFrame.BorderSizePixel = 0
LoginFrame.Position = UDim2.new(0.389049619, 0, 0.253605783, 0)
LoginFrame.Size = UDim2.new(0.298493832, 0, 0.412259609, 0)
LoginFrame.Visible = true
LoginFrame.Active = true
LoginFrame.Draggable = true

LoginFrame.MouseButton1Down:connect(function()
	if password.Text =="Phum" or "Dew" then
		LoginFrame.Visible = false
		XlFrame.Visible = true
		DisFrame.Visible = true
	end
end)

