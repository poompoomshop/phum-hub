Cikme.MouseButton1Down:connect(function()
	LoginFrame.Visible = false --- false = hide 
	MenuFrame.Visible = true--- true = dont hide 
	BigFrame.Visible = true
end)

Add.MouseButton1Down:connect(function()
	MenuFrame.Visible = true
	Exit.Visible = false
	Add.Visible = false
end)

Exit.MouseButton1Down:connect(function()
	MenuFrame.Visible = false
	Add.Visible = true
	Exit.Visible = false
end)

Pan.MouseButton1Down:connect(function()
	ScrollingFrame.Visible = true
end)

bypass.MouseButton1Down:connect(function()
